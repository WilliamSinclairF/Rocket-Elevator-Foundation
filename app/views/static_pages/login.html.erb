<section>
  <div class="container">
    <div class="text-center">
      <div class="m-20">
        <img  src="https://raw.githubusercontent.com/WilliamSinclairF/RocketElevatorsDigitalPresence-frontend/master/src/assets/RE_assets/RE_logo.png" alt=""  height="300">
      </div>
      <div class="m-20">
        <h1>Log In</h1>
      </div>
    </div>
    <form class="form-signin d-flex justify-content-center flex-wrap">
      <div class="w-100p m-5">
        <label for="inputEmail" class="sr-only">Email address</label>
        <input type="email" id="inputEmail" class="form-control w-75p ml-auto mr-auto" placeholder="Email address" required="true" autofocus="true">
      </div>
      <div class="w-100p m-5">
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" class="form-control w-75p ml-auto mr-auto" placeholder="Password" required="true">
      </div>
    </form>
    <div>
      <div class="d-flex justify-content-center m-5">
        <label for="remember-me" class="fs-25 unselectable">
          <input id="remember-me" type="checkbox" value="remember-me">
          <strong>Remember me</strong>
        </label>
      </div>
      <div class="d-flex justify-content-center">
        <button class="btn btn-lg btn-primary btn-block w-50p" type="submit">Log in</button>
      </div>
    </div>
  </div>
</section>
