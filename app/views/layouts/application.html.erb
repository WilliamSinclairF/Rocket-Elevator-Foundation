<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <%= stylesheet_link_tag    'essentials', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'layout', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'custom', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'header', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'red', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'extralayers', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'settings', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'bootstrap.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700" rel="stylesheet" type="text/css" />
  </head>

  <body>
    <%= yield %>

    <%= javascript_include_tag 'contact.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'quote.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'scripts.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery-3.3.1.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.themepunch.tools.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.themepunch.revolution.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'demo.revolution_slider.js', 'data-turbolinks-track': 'reload' %>

  </body>
</html>
